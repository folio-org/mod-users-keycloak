    "jsonBody": {
      "id": "00000000-1000-0000-0000-000000000000",
      "name": "default-system-role-mod-foo",
      "description": "Default system role for mod-foo",
      "permissions": [
        {
          "roleId": "00000000-1000-0000-0000-000000000000",

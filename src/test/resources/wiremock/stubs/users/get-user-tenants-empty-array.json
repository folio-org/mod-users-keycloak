{
  "request": {
    "method": "GET",
    "urlPath": "/user-tenants",
    "headers": {
      "x-okapi-tenant": {
        "equalTo": "centraltenant"
      },
      "x-okapi-token": {
        "equalTo": "X-Okapi-Token test value"
      }
    },
    "queryParameters": {
      "userId": {
        "matches": ".*"
      },
      "limit": {
        "equalTo": "1"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "userTenants": [],
      "totalRecords": 0
    }
  }
}
